<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd"> 
                <context:annotation-config></context:annotation-config>
               <bean id="fuel" class="com.requeried.annotations.Fuel">
               <property name="fuelType" value="diesal"></property>
               <property name="capacity" value="20"></property>
               </bean>
               <bean id="motor" class="com.requeried.annotations.Motor">
<!--               <property name="fuel" ref="fuel"></property> -->
               </bean>
            
               </beans>